{
  "name": "tailwind-parcel-project",
  "version": "1.0.0",
  "description": "Proyecto que utiliza Tailwind CSS y Parcel",
  "scripts": {
    "dev": "parcel src/index.html && parcel src/index.html",
    "build": "parcel build src/index.html"
  },
  "keywords": [
    "tailwind",
    "parcel",
    "css",
    "html",
    "javascript"
  ],
  "author": "El teu nom",
  "license": "ISC",
  "dependencies": {
    "@parcel/transformer-postcss": "^2.10.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "tailwindcss": "^3.3.3"
  },
  "parcel": {
    "sourceMap": false,
    "extends": "@parcel/config-default",
    "plugins": [
      {
        "name": "preset-default"
      },
      "cleanupIDs"
    ]
  }
}
